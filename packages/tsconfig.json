{
    "extends": "./base.tsconfig",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@theia/core/lib/*": [
                "core/src/*"
            ],
            "@theia/filesystem/lib/*": [
                "filesystem/src/*"
            ],
            "mv": [
                "filesystem/src/typings/mv"
            ],
            "trash": [
                "filesystem/src/typings/trash"
            ]
        }
    },
    "include": [
        "*/src"
    ]
}