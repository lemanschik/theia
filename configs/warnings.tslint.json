{
  "rules": {
    "await-promise": {
      "severity": "warning",
      "options": [
        "Thenable",
        "PromiseLike"
      ]
    },
    "deprecation": true,
    "no-return-await": {
      "severity": "warning"
    },
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ]
  }
}
